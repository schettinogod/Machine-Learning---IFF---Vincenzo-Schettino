      "source": [
        "# Percorrer cada tweet no DataFrame\n",
        "for i, tweet in enumerate(df['tweet']):\n",
        "    # Verificar se uma URL est√° presente no tweet usando express√£o regular\n",
        "    possui_url = bool(re.search('https?://(?:[-\\w.]|(?:%[\\da-fA-F]{2}))+', tweet))\n",
        "\n",
        "    # Armazenar 1 se uma URL est√° presente e 0 caso contr√°rio na coluna \"possui_url\" para o tweet correspondente\n",
        "    dataset.loc[i, 'URL'] = 1 if possui_url else 0"
