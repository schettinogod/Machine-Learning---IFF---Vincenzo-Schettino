        "    num_pontos_interrogacao = tweet.count('?')\n",
        "\n",
        "    # Armazenar o n√∫mero de pontos de interroga√ß√£o na coluna \"num_pontos_interrogacao\" para o tweet correspondente\n",
        "    dataset.loc[i, '?-Quantidade'] = num_pontos_interrogacao"
      ],
      "metadata": {
        "id": "HSdzb559PTWy"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "for i, tweet in enumerate(df['tweet']):\n",
        "    # Dividir o tweet em palavras separadas por espa√ßo\n",
        "    palavras = tweet.split()\n",
        "\n",
        "    # Calcular o n√∫mero total de caracteres e o n√∫mero total de palavras\n",
        "    num_caracteres_total = sum(len(palavra) for palavra in palavras)\n",
        "    num_palavras = len(palavras)\n",
        "\n",
        "    # Calcular a m√©dia de caracteres por palavra\n",
        "    if num_palavras > 0:\n",
        "        media_caracteres_por_palavra = num_caracteres_total / num_palavras\n",
        "    else:\n",
        "        media_caracteres_por_palavra = 0.0\n",
        "\n",
        "    # Armazenar a m√©dia de caracteres por palavra na coluna \"media_caracteres_por_palavra\" para o tweet correspondente\n",
        "    dataset.loc[i, 'Media-Caracteres'] = media_caracteres_por_palavra\n"
      ],
      "metadata": {
        "id": "yvt514LrSTzQ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "for i, tweet in enumerate(df['tweet']):\n",
        "    # Definir os emojis positivos que voc√™ deseja contar\n",
        "    emojis_positivos = [':)', 'üòÉ', 'üòÑ', 'üòä', 'üòÅ']\n",
        "\n",
        "    # Contar a quantidade de emojis positivos presentes no tweet usando express√µes regulares\n",
        "    regex = '|'.join(re.escape(em) for em in emojis_positivos)\n",
        "    qtde_emojis_positivos = len(re.findall(regex, tweet))\n",
        "\n",
        "    # Armazenar a quantidade de emojis positivos na coluna \"qtde_emojis_positivos\" para o tweet correspondente\n",
        "    dataset.loc[i, 'Emoji-Positivo-Quant'] = qtde_emojis_positivos"
      ],
      "metadata": {
        "id": "6k0Eb6PnZNVY"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "for i, tweet in enumerate(df['tweet']):\n",
        "    # Definir os emojis negativos que voc√™ deseja contar\n",
        "    emojis_negativos = [':(', 'üòû', 'üòî', 'üôÅ', 'üò¢']\n",
        "\n",
        "    # Contar a quantidade de emojis negativos presentes no tweet usando express√µes regulares\n",
        "    regex = '|'.join(re.escape(em) for em in emojis_negativos)\n",
        "    qtde_emojis_negativos = len(re.findall(regex, tweet))\n",
        "\n",
        "    # Armazenar a quantidade de emojis negativos na coluna \"qtde_emojis_negativos\" para o tweet correspondente\n",
        "    dataset.loc[i, 'Emoji-Negativo-Quant'] = qtde_emojis_negativos"
      ],
