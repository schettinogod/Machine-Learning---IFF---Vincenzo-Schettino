        "for i, tweet in enumerate(df['tweet']):\n",
        "    # Definir os emojis positivos que voc√™ deseja procurar\n",
        "    emojis_positivos = [':)', 'üòÉ', 'üòÑ', 'üòä', 'üòÅ']\n",
        "\n",
        "    # Verificar se algum emoji positivo est√° presente no tweet usando express√µes regulares\n",
        "    regex = '|'.join(re.escape(em) for em in emojis_positivos)\n",
        "    possui_emoji_positivo = bool(re.search(regex, tweet))\n",
        "\n",
        "    # Armazenar 1 se um emoji positivo est√° presente e 0 caso contr√°rio na coluna \"possui_emoji_positivo\" para o tweet correspondente\n",
        "    dataset.loc[i, 'Emoji-Positivo'] = 1 if possui_emoji_positivo else 0\n"
      ],
      "metadata": {
